{
  "name": "dusky",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:all": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/run-tests.ps1",
    "bdd": "cucumber-js tests/features/**/*.feature --require-module ts-node/register --require tests/steps/**/*.ts",
    "bdd:addjob": "cucumber-js tests/features/AddJob.feature --require-module ts-node/register --require tests/steps/**/*.ts",
    "bdd:schedulejob": "cucumber-js tests/features/schedulejob.feature --require-module ts-node/register --require tests/steps/**/*.ts",
    "bdd:addjob-schedulejob": "cucumber-js \"tests/features/AddJob&ScheduleJob.feature\" --require-module ts-node/register --require tests/steps/**/*.ts",
    "pdf:edit": "ts-node tests/pages/pdfeditor.ts",
    "pdf:create": "ts-node tests/pages/pdfeditor.ts create",
    "login:setup": "ts-node --project ./tsconfig.json ./tests/loginsetup/loginstepup.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A comprehensive Playwright-based test automation framework for web application testing with a clean, maintainable architecture.",
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.0",
    "@types/pdf-parse": "^1.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "libreoffice-convert": "^1.7.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1"
  },
  "directories": {
    "test": "tests"
  }
}
